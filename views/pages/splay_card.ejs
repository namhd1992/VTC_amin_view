<body class="page-container-bg-solid page-header-fixed page-sidebar-closed-hide-logo">
	<% include ../partials/header %>
		<!-- BEGIN HEADER & CONTENT DIVIDER -->
		<div class="clearfix"></div>
		<!-- END HEADER & CONTENT DIVIDER -->
		<!-- BEGIN CONTAINER -->
		<div class="page-container">
			<% include ../partials/side_bar %>
				<!-- BEGIN CONTENT -->
				<div class="page-content-wrapper">
					<!-- BEGIN CONTENT BODY -->
					<div class="page-content">
						<% include ../partials/page_title %>
							<!-- BEGIN PAGE BREADCRUMB -->
							<ul class="page-breadcrumb breadcrumb">
								<li>
									<a href="./">Dashboard</a>
									<i class="fa fa-circle"></i>
								</li>
							</ul>
							<div class="portlet light bordered">
								<div class="portlet-title">
									<div class="caption font-dark">
										<i class="icon-settings font-dark"></i>
										<span class="caption-subject bold uppercase">Splay card</span>
									</div>
								</div>
								<div class="portlet-body">
									<div class="search_wrap">
										<div class="input-group">
											<button class="btn sbold green btn_add_new_item"> Add New
												<i class="fa fa-plus"></i>
											</button>
										</div>
										<div class="input-group">
											<input type="text" class="search_input" placeholder="Search.." />
										</div>
										<div class="input-group">
											<button class="search_submit btn btn-success">Apply</button>
											<button class="search_clear btn" style="margin-left:10px;">Clear</button>
										</div>
									</div>
									<table class="table table-striped table-bordered table-hover table-checkable order-column" id="page_datatable"></table>
									<div class="datatable_paging"></div>
								</div>
								<!--confirm modal-->
								<div id="modal_confirm" class="modal fade modal-sm" tabindex="-1" data-width="420">
									<div class="modal-header">
										<button type="button" class="close" data-dismiss="modal" aria-hidden="true"></button>
										<h4 class="modal-title"></h4>
									</div>
									<div class="modal-body">
										Are you sure?
									</div>
									<div class="modal-footer">
										<button type="button" class="btn green btn_confirm_yes">Yes</button>
										<button type="button" data-dismiss="modal" class="btn btn-outline dark">No</button>
									</div>
								</div>
								<!--modal add edit-->
								<div id="modal_add_edit" class="modal fade modal-scroll" tabindex="-1" data-width="90%">
									<div class="modal-header">
										<button type="button" class="close" data-dismiss="modal" aria-hidden="true"></button>
										<h4 class="modal-title">Create New Tag</h4>
									</div>
									<div class="modal-body">
										<div class="portlet-body form">
											<!-- BEGIN FORM-->
											<form id="add_giftcode_form" action="#" class="form-horizontal" enctype="multipart/form-data">
												<div class="form-body">
													<div class="form-group">
														<label class="col-md-3 control-label">Giftcode *</label>
														<div class="col-md-6">
															<input type="file" name="fileCard" class="file_add_edit_item_giftcode">
															<p class="help-block"></p>
														</div>
													</div>
												</div>
											</form>
										</div>
									</div>
									<div class="modal-footer">
										<button type="button" data-dismiss="modal" class="btn btn-outline dark">Close</button>
										<button type="button" class="btn green btn_modal_save_new">Add</button>
										<button type="button" class="btn green btn_modal_save_update">Save Change</button>
									</div>
								</div>

							</div>
					</div>
					<!-- END CONTENT BODY -->
				</div>
				<!-- END CONTENT -->
				<!-- BEGIN QUICK SIDEBAR -->
				<!-- END QUICK SIDEBAR -->
