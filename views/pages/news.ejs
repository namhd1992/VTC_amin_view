<body class="page-container-bg-solid page-header-fixed page-sidebar-closed-hide-logo">
    <% include ../partials/header %>
        <!-- BEGIN HEADER & CONTENT DIVIDER -->
        <div class="clearfix"> </div>
        <!-- END HEADER & CONTENT DIVIDER -->
        <!-- BEGIN CONTAINER -->
        <div class="page-container">
            <% include ../partials/side_bar %>
                <!-- BEGIN CONTENT -->
                <div class="page-content-wrapper">
                    <!-- BEGIN CONTENT BODY -->
                    <div class="page-content">
                        <% include ../partials/page_title %>
                            <!-- BEGIN PAGE BREADCRUMB -->
                            <ul class="page-breadcrumb breadcrumb">
                                <li>
                                    <a href="./">Dashboard</a>
                                    <i class="fa fa-circle"></i>
                                </li>
                            </ul>
                            <div class="portlet light bordered">
                                <div class="portlet-title">
                                    <div class="caption font-dark">
                                        <i class="icon-settings font-dark"></i>
                                        <span class="caption-subject bold uppercase">Articles</span>
                                    </div>
                                </div>
                                <!--<button class="btn sbold green btn_add_new_item"> Add New <i class="fa fa-plus"></i></button>-->
                                <div class="portlet-body">
                                    <div class="table-toolbar">
                                        <div class="row">
                                            <div class="col-md-6">
                                                <div class="btn-group">
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <table class="table table-bordered table-checkable order-column" id="page_datatable"></table>
                                </div>

                                <!--confirm modal-->
                                <div id="modal_confirm" class="modal fade modal-sm" tabindex="-1" data-width="420">
                                    <div class="modal-header">
                                        <button type="button" class="close" data-dismiss="modal" aria-hidden="true"></button>
                                        <h4 class="modal-title"></h4>
                                    </div>
                                    <div class="modal-body">
                                        Are you sure?
                                    </div>
                                    <div class="modal-footer">
                                        <button type="button" class="btn green btn_confirm_yes">Yes</button>
                                        <button type="button" data-dismiss="modal" class="btn btn-outline dark">No</button>
                                    </div>
                                </div>

                                <!--modal add edit-->
                                <div id="modal_add_edit" class="modal fade modal-scroll" tabindex="-1" data-width="760">
                                    <div class="modal-header">
                                        <button type="button" class="close" data-dismiss="modal" aria-hidden="true"></button>
                                        <h4 class="modal-title">Edit Article Info</h4>
                                    </div>
                                    <div class="modal-body">
                                        <div class="portlet-body form">
                                            <!-- BEGIN FORM-->
                                            <form action="#" class="form-horizontal">
                                                <div class="form-body">
                                                    <div class="form-group">
                                                        <label class="col-md-3 control-label">Title</label>
                                                        <div class="col-md-8">
                                                            <textarea class="txt_add_edit_item_name form-control" placeholder="Title ..."></textarea>
                                                        </div>
                                                    </div>
                                                    <div class="form-group">
                                                        <label class="col-md-3 control-label">Image </label>
                                                        <div class="col-md-8">
                                                            <input type="text" class="txt_add_edit_item_img form-control" placeholder="Enter url">
                                                        </div>
                                                    </div>
                                                    <div class="form-group">
                                                        <label class="col-md-3 control-label">Status</label>
                                                        <div class="col-md-4">
                                                            <select class="bs-select form-control input-small select_status" data-style=" btn-success">
                                                            <option value="active">Active</option>
                                                            <option value="inactive">Inactive</option>
                                                        </select>
                                                        </div>
                                                    </div>
                                                </div>
                                            </form>
                                        </div>
                                        <!--<form action="#" id="form-username" class="form-horizontal form-bordered">
                                        <div class="form-group">
                                            <label class="col-sm-1 control-label">Tags</label>
                                            <div class="col-sm-4">
                                                <div class="input-group">
                                                    <span class="input-group-addon">
                                                        <i class="fa fa-search"></i>
                                                    </span>
                                                    <input type="text" id="typeahead_example_1" name="typeahead_example_1" class="form-control" /> </div>
                                                <p class="help-block"> E.g: MOBA, ESPORTS</p>
                                            </div>
                                        </div>
                                    </form>-->
                                    </div>
                                    <div class="modal-footer">
                                        <button type="button" data-dismiss="modal" class="btn btn-outline dark">Close</button>
                                        <button type="button" class="btn green btn_modal_save_update">Save Change</button>
                                    </div>
                                </div>
                            </div>
                    </div>
                    <!-- END CONTENT BODY -->
                </div>
                <!-- END CONTENT -->
                <!-- BEGIN QUICK SIDEBAR -->
                <!-- END QUICK SIDEBAR -->
        </div>
        <!-- END CONTAINER -->
        <!-- BEGIN FOOTER -->
        <% include ../partials/footer %>
            <!-- END FOOTER -->
