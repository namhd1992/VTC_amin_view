<body class="page-container-bg-solid page-header-fixed page-sidebar-closed-hide-logo">
    <% include ../partials/header %>
        <!-- BEGIN HEADER & CONTENT DIVIDER -->
        <div class="clearfix"> </div>
        <!-- END HEADER & CONTENT DIVIDER -->
        <!-- BEGIN CONTAINER -->
        <div class="page-container">
            <% include ../partials/side_bar %>
                <!-- BEGIN CONTENT -->
                <div class="page-content-wrapper">
                    <!-- BEGIN CONTENT BODY -->
                    <div class="page-content">
                        <% include ../partials/page_title %>
                            <!-- BEGIN PAGE BREADCRUMB -->
                            <ul class="page-breadcrumb breadcrumb">
                                <li>
                                    <a href="./">Dashboard</a>
                                    <i class="fa fa-circle"></i>
                                </li>
                            </ul>
                            <div class="portlet light bordered">
                                <div class="portlet-title">
                                    <div class="caption font-dark">
                                        <i class="icon-settings font-dark"></i>
                                        <span class="caption-subject bold uppercase">Channels</span>
                                    </div>
                                </div>
                                <div class="search_wrap">
                                    <div class="input-group">
                                        <button class="btn_add_new_item btn sbold green"> Add New <i class="fa fa-plus"></i></button>
                                    </div>
                                    <div class="input-group">
                                        <button class="btn_update_position btn sbold blue" style="margin-left: 5px;float:left;"> Position <i class="fa fa-reorder"></i></button>
                                    </div>
                                    <div class="input-group">
                                        <input type="text" class="search_input" />
                                    </div>
                                    <div class="input-group">
                                        <button class="search_submit btn btn-success">Apply</button>
                                        <button class="search_clear btn" style="margin-left:10px;">Clear</button>
                                    </div>
                                </div>
                                <div class="portlet-body">
                                    <table class="table table-striped table-bordered table-hover table-checkable order-column" id="page_datatable"></table>
                                    <div class="datatable_paging"></div>
                                </div>

                                <!--confirm modal-->
                                <div id="modal_confirm" class="modal fade modal-sm" tabindex="-1" data-width="420">
                                    <div class="modal-header">
                                        <button type="button" class="close" data-dismiss="modal" aria-hidden="true"></button>
                                        <h4 class="modal-title"></h4>
                                    </div>
                                    <div class="modal-body">
                                        Are you sure?
                                    </div>
                                    <div class="modal-footer">
                                        <button type="button" class="btn green btn_confirm_yes">Yes</button>
                                        <button type="button" data-dismiss="modal" class="btn btn-outline dark">No</button>
                                    </div>
                                </div>

                                <!-- add/edit modal -->
                                <div id="modal_add_edit" class="modal fade modal-scroll" tabindex="-1" data-width="760">
                                    <div class="modal-header">
                                        <button type="button" class="close" data-dismiss="modal" aria-hidden="true"></button>
                                        <h4 class="modal-title"></h4>
                                    </div>
                                    <div class="modal-body">
                                        <form action="#" class="form-horizontal">
                                            <div class="form-body">
                                                <div class="form-group">
                                                    <label class="col-md-3 control-label">Name</label>
                                                    <div class="col-md-8">
                                                        <input required type="text" class="form-control txt_add_edit_channel_name" placeholder="Enter text">
                                                    </div>
                                                </div>

                                                <div class="form-group">
                                                    <label class="col-md-3 control-label">Image URL</label>
                                                    <div class="col-md-8">
                                                        <input type="text" class="form-control txt_add_edit_channel_image" placeholder="Enter URL">
                                                    </div>
                                                </div>
                                                <div class="form-group">
                                                    <label class="col-md-3 control-label">Tags</label>
                                                    <div class="col-md-6">
                                                        <select id="single_select" class="form-control select2">
                                                            <option></option>
                                                            
                                                        </select>
                                                    </div>
                                                </div>
                                                <div class="form-group">
                                                    <label class="col-md-3 control-label"></label>
                                                    <div class="col-md-8">
                                                        <div class="current_tags bootstrap-tagsinput">
                                                        </div>
                                                    </div>
                                                </div>
                                                <!--<div class="form-group">
                                                    <label class="col-md-3 control-label">Auto Follows</label>
                                                    <div class="col-md-6">
                                                        <select id="single_select_packages" class="form-control select2">
                                                            <option></option>
                                                        </select>
                                                    </div>
                                                </div>
                                                <div class="form-group">
                                                    <label class="col-md-3 control-label"></label>
                                                    <div class="col-md-8">
                                                        <div class="current_packages bootstrap-tagsinput">
                                                        </div>
                                                    </div>
                                                </div>-->
                                                <div class="form-group">
                                                    <label class="col-md-3 control-label">Status</label>
                                                    <div class="col-md-4">
                                                        <select class="bs-select form-control input-small select_status" data-style=" btn-success">
                                                            <option value="active">Active</option>
                                                            <option value="inactive">Inactive</option>
                                                        </select>
                                                    </div>
                                                </div>
                                            </div>
                                            <!--<div class="form-group">
                                                <label class="col-md-3 control-label"></label>
                                                <div class="col-md-4">
                                                    <a class='btn_delete_item' item_id=''><i class='icon-trash'></i> Delete </a>
                                                </div>
                                            </div>-->
                                        </form>
                                    </div>
                                    <div class="modal-footer">
                                        <button type="button" data-dismiss="modal" class="btn btn-outline dark">Close</button>
                                        <button type="button" class="btn green btn_modal_save_new">Add</button>
                                        <button type="button" class="btn green btn_modal_save_update">Save Change</button>
                                    </div>
                                </div>

                                <!--update position modal-->
                                <div id="modal_update_position" class="modal fade modal-scroll" tabindex="-1" data-width="760">
                                    <div class="modal-header">
                                        <button type="button" class="close" data-dismiss="modal" aria-hidden="true"></button>
                                        <h4 class="modal-title">Update Position</h4>
                                    </div>
                                    <div class="modal-body">
                                        <ul id="items">
                                        </ul>
                                    </div>
                                    <div class="modal-footer">
                                        <button type="button" data-dismiss="modal" class="btn btn-outline dark">Close</button>
                                        <button type="button" class="btn green btn_modal_save_position">Save Change</button>
                                    </div>
                                </div>
                            </div>
                    </div>
                    <!-- END CONTENT BODY -->
                </div>
                <!-- END CONTENT -->
        </div>
        <!-- END CONTAINER -->
        <% include ../partials/footer %>
            <!--[if lt IE 9]>
            <script src="../assets/global/plugins/respond.min.js"></script>
