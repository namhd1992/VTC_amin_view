<body class="page-container-bg-solid page-header-fixed page-sidebar-closed-hide-logo">
    <% include ../partials/header %>
        <!-- BEGIN HEADER & CONTENT DIVIDER -->
        <div class="clearfix"> </div>
        <!-- END HEADER & CONTENT DIVIDER -->
        <!-- BEGIN CONTAINER -->
        <div class="page-container">
            <% include ../partials/side_bar %>
                <!-- BEGIN CONTENT -->
                <div class="page-content-wrapper">
                    <!-- BEGIN CONTENT BODY -->
                    <div class="page-content">
                        <% include ../partials/page_title %>
                            <!-- BEGIN PAGE BREADCRUMB -->
                            <ul class="page-breadcrumb breadcrumb">
                                <li>
                                    <a href="./">Dashboard</a>
                                    <i class="fa fa-circle"></i>
                                </li>
                            </ul>

                            <!--General-->
                            <div class="portlet light bordered">
                                <div class="portlet-title">
                                    <div class="caption font-dark">
                                        <i class="icon-settings font-dark"></i>
                                        <span class="caption-subject bold uppercase">General Settings</span>
                                    </div>
                                </div>
                                <div class="portlet-body">
                                    <form action="" class="form-horizontal form_general">
                                        <div class="form-body">
                                            <div class="col-md-6"><label class="lb_cb_active"><input type="checkbox" class="cb_deposit"  value="" />Deposit (<span class="cb_deposit_text"></span>)</label></div>
                                            <div class="col-md-6"><label class="lb_cb_active"><input type="checkbox" class="cb_transfer"  value="" />Transfer (<span class="cb_transfer_text"></span>)</label></div>
                                        </div>
                                    </form>
                                </div>
                            </div>

                            <!--Transfer-->
                            <div class="portlet light bordered">
                                <div class="portlet-title">
                                    <div class="caption font-dark">
                                        <i class="icon-settings font-dark"></i>
                                        <span class="caption-subject bold uppercase">Transfer Settings</span>
                                    </div>
                                </div>
                                <div class="portlet-body">
                                    <form action="" class="form-horizontal form_general">
                                        <div class="form-body">
                                            <div class="form-group">
                                                <label class="col-md-4 control-label">Max transfer per day</label>
                                                <div class="col-md-6">
                                                    <input type="number" min="1" class="txt_max_transfer_day form-control" name="" placeholder="">
                                                    <p class="help-block">Số lần giao dịch tối đa 1 ngày</p>
                                                </div>
                                            </div>
                                            <div class="form-group">
                                                <label class="col-md-4 control-label">Max coin transfer per day</label>
                                                <div class="col-md-6">
                                                    <input type="number" min="1" class="txt_max_coin_day form-control" name="" placeholder="">
                                                    <p class="help-block">Số tiền giao dịch tối đa 1 ngày</p>
                                                </div>
                                            </div>
                                            <div class="form-group">
                                                <label class="col-md-4 control-label">Max coin transfer per time</label>
                                                <div class="col-md-6">
                                                    <input type="number" min="1" class="txt_max_coin_time form-control" name="" placeholder="">
                                                    <p class="help-block">Số tiền giao dịch tối đa 1 lần</p>
                                                </div>
                                            </div>
                                            <div class="form-group">
                                                <label class="col-md-4 control-label">Max fail transfer per day</label>
                                                <div class="col-md-6">
                                                    <input type="number" min="1" class="txt_max_fail_day form-control" name="" placeholder="">
                                                    <p class="help-block">Số lần giao dịch lỗi tối đa 1 ngày</p>
                                                </div>
                                            </div>
                                            <div class="form-group">
                                                <label class="col-md-4 control-label">Time between 2 transaction</label>
                                                <div class="col-md-6">
                                                    <input type="number" min="1" class="txt_time_between form-control" name="" placeholder="">
                                                    <p class="help-block">Khoảng cách giữa 2 lần giao dịch (Phút)</p>
                                                </div>
                                            </div>
                                            <div class="form-group">
                                                <label class="col-md-4 control-label" style="text-align:left;font-weight: bold;">Transfer fee</label>
                                            </div>
                                            <div class="form-group">
                                                <div class="col-md-12">
                                                    <table class="table table-striped table-bordered table-hover table_transfer_fee">
                                                        <thead>
                                                            <th>From</th>
                                                            <th>To</th>
                                                            <th>Fee(%)</th>
                                                            <th>Fee(min)</th>
                                                            <th>
                                                                <div class="tooltip_help">?
                                                                    <div class="tooltiptext_help">
                                                                        - Fee(%): Phí theo % số tiền chuyển. <br/>- Min fee: Phí nhỏ nhất cho phép (nếu phí tính theo % nhỏ hơn min fee thì sẽ tính phí theo min fee). <br/>- Set Fee(%) bằng 0, để cố định
                                                                        phí theo min fee. <br/>- Set min fee bằng 0 để luôn tính phí theo %.<br/>- Hệ thống sẽ tự sắp sếp thứ tự khi lưu.
                                                                    </div>
                                                                </div>
                                                            </th>
                                                        </thead>
                                                        <tbody>
                                                            <tr class="input_row">
                                                                <td><input type="number" style="width:100%;" value=0 class="txt_transfer_fee_from" /></td>
                                                                <td><input type="number" style="width:100%;" value=0 class="txt_transfer_fee_to" /></td>
                                                                <td><input type="number" style="width:100%;" value=0 class="txt_transfer_fee_percent" /></td>
                                                                <td><input type="number" style="width:100%;" value=0 class="txt_transfer_fee_min" /></td>
                                                                <td>
                                                                    <div class="btn btn-link btn_transfer_add">Add</div>
                                                                </td>
                                                            </tr>
                                                        </tbody>
                                                    </table>
                                                </div>
                                            </div>
                                        </div>
                                    </form>
                                </div>
                            </div>
                            <!--Transfer-->
                            <div class="portlet light bordered">
                                <div class="portlet-title">
                                    <div class="caption font-dark">
                                        <i class="icon-settings font-dark"></i>
                                        <span class="caption-subject bold uppercase">Deposit Settings</span>
                                    </div>
                                </div>
                                <div class="portlet-body">
                                    <form action="" class="form-horizontal form_general">
                                        <div class="form-body">
                                            <div class="form-group">
                                                <div class="col-md-12">
                                                    <table class="table table-striped table-bordered table-hover table_deposit" style="width: 60%;">
                                                        <thead>
                                                            <th>S-Coin</th>
                                                            <th>League Point</th>
                                                            <th></th>
                                                        </thead>
                                                        <tbody>
                                                            <tr class="input_row">
                                                                <td><input type="number" style="width:100%;" value=0 class="txt_deposit_coin" /></td>
                                                                <td><input type="number" style="width:100%;" value=0 class="txt_deposit_price" /></td>
                                                                <td>
                                                                    <div class="btn btn-link btn_deposit_add">Add</div>
                                                                </td>
                                                            </tr>
                                                        </tbody>
                                                    </table>
                                                </div>
                                            </div>
                                        </div>
                                    </form>
                                </div>
                            </div>

                            <!--Save button-->
                            <div class="portlet light bordered">
                                <div class="portlet-body">
                                    <form action="" class="form-horizontal form_general">
                                        <div class="form-body">
                                            <div class="form-group">
                                                <div class="col-md-12" style="text-align:right;">
                                                    <div class="btn btn-success btn_save_setting">Save</div>
                                                </div>
                                            </div>
                                        </div>
                                    </form>
                                </div>
                            </div>
                    </div>

                    <!--confirm modal-->
                    <div id="modal_confirm" class="modal fade modal-sm" tabindex="-1" data-width="420">
                        <div class="modal-header">
                            <button type="button" class="close" data-dismiss="modal" aria-hidden="true"></button>
                            <h4 class="modal-title"></h4>
                        </div>
                        <div class="modal-body">
                            Are you sure?
                        </div>
                        <div class="modal-footer">
                            <button type="button" class="btn green btn_confirm_yes">Yes</button>
                            <button type="button" data-dismiss="modal" class="btn btn-outline dark">No</button>
                        </div>
                    </div>
                    <!-- END CONTENT BODY -->
                </div>
                <!-- END CONTENT -->
                <!-- BEGIN QUICK SIDEBAR -->
                <!-- END QUICK SIDEBAR -->
        </div>

        <!-- END CONTAINER -->
        <!-- BEGIN FOOTER -->
        <% include ../partials/footer %>
            <!-- END FOOTER -->
