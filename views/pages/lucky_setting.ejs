                                                <div class="col-md-4">
                                                    <input type="number" class="txt_turn_medium form-control">
                                                </div>
                                            </div>
                                            <div class="form-group bet_price_group">
                                                <label class="col-md-3 control-label">Buy turn big</label>
                                                <div class="col-md-4">
                                                    <input type="number" class="txt_turn_big form-control">
                                                </div>
                                            </div>
                                            <div class="form-group bet_price_group">
                                                <label class="col-md-3 control-label">Rule</label>
                                                <div class="col-md-6">
                                                    <textarea class="txt_lucky_role form-control"></textarea>
                                                </div>
                                            </div>

                                            <div class="form-group">
                                                <label class="col-xs-3 control-label"></label>
                                                <div class="col-xs-9">
                                                    <input class="btn sbold green btn_submit" type="button" value="Save" />
                                                </div>
                                            </div>
                                        </div>
